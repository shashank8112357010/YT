# Production Environment Variables for /root/techassistant/YT
NODE_ENV=production
PORT=3008

# CORS Configuration
ALLOWED_ORIGINS=https://yt.techassistant.co.in,http://yt.techassistant.co.in

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30

# Security Headers
CSP_REPORT_URI=https://yt.techassistant.co.in/api/csp-report

# Logging
LOG_LEVEL=error

# Content Limits
MAX_REQUEST_SIZE=10mb
MAX_URL_LENGTH=2048
MAX_TABS_PER_REQUEST=10

# Session Security
SESSION_SECRET=your-very-long-random-production-string-change-this-12345
SECURE_COOKIES=true

# Monitoring
HEALTH_CHECK_TOKEN=your-production-health-check-token-12345
